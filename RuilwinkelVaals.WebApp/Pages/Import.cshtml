@page
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model RuilwinkelVaals.WebApp.Pages.ImportModel
@{
    ViewData["Title"] = "Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<a href="~/Files/ImportTable.csv">Download Template</a>

    <form method="post" enctype="multipart/form-data">
        <input id="uploadFile" type="file" asp-for="uploadFile" />
        <input id="FileUploadButton" type="submit" value="Upload" asp-page-handler="fileButtonPressed" />
    </form>
<pre>@Model.feedback</pre>
