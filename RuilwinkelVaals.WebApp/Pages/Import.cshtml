@page
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model RuilwinkelVaals.WebApp.Pages.ImportModel
@{
    ViewData["Title"] = "Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form method="post" enctype="multipart/form-data">
    <input id="uploadFile" type="file" asp-for="uploadFile" />
    <input id="FileUploadButton" type="submit" value="button" asp-page-handler="fileButtonPressed" />
</form>